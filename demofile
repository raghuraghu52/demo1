pipeline {
    agent any
   
    stages {
        stage(clone) {
            steps {
                 git "https://github.com/raghuraghu52/demo1.git"
               
            }
        }
                 stage(build) {
            steps {
                sh "mvn clean install -Dskiptests"
            }
          }
          stage(test) {
            steps {
                 sh "mvn test"
            }
          }
    }
}
